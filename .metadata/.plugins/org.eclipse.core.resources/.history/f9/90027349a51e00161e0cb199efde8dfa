package com.filipe.cursojava.aula24;

public class TesteCarro {

	public static void main(String[] args) {
		
		Carro van = new Carro();
		
		van.marca = "Fiat";
		van.modelo = "Ducato";
		van.numeroDePassageiros = 10;
		van.capacidadeDeCombustvel = 100;
		van.comsumoDeCombustivel = 0.2;
		
		System.out.println(van);
	}

}
